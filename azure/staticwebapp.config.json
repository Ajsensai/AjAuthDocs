{
    "routes": [{
        "route": "/.auth/login/aad",
        "statusCode": "401"
      },
      {
        "route": "/.auth/logout",
        "statusCode": "302"
      },
      {
        "route": "/api/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/.auth/*"]
    },
    "globalHeaders": {
      "Content-Security-Policy": "default-src 'self' 'unsafe-inline'; img-src *; font-src *"
    }
  }
  